doctype html
head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  link(rel='stylesheet' href='/stylesheets/register/signup.css')
  link(rel="stylesheet" href="https://cdn.simplecss.org/simple.min.css")
  title Home
if sortedSchemas
  .schemas
    each schemas, ranking in sortedSchemas 
      //- unless ranking=="low"
      div(class="ranking" id=ranking)
        h2= ranking
        each schema, name in schemas
          .schema
            a(href=schema["urlLink"])=schema["schema"] + ":" + schema["value"]
            p(class='description')=descriptions[name]
        br